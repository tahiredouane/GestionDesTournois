{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Equipes</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Association</th>
                <td>{{ entity.association }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
                <th>Coach</th>
                <td>{{ entity.coach }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ entity.cin }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ entity.photo }}</td>
            </tr>
            <tr>
                <th>Photoequipe</th>
                <td>{{ entity.photoEquipe }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entity.logo }}</td>
            </tr>
            <tr>
                <th>Lienphoto</th>
                <td>{{ entity.lienPhoto }}</td>
            </tr>
            <tr>
                <th>Lienphotoequipe</th>
                <td>{{ entity.lienPhotoEquipe }}</td>
            </tr>
            <tr>
                <th>Lienlogo</th>
                <td>{{ entity.lienLogo }}</td>
            </tr>
            <tr>
                <th>Orders</th>
                <td>{{ entity.orders }}</td>
            </tr>
            <tr>
                <th>Str1</th>
                <td>{{ entity.str1 }}</td>
            </tr>
            <tr>
                <th>Str2</th>
                <td>{{ entity.str2 }}</td>
            </tr>
            <tr>
                <th>Str3</th>
                <td>{{ entity.str3 }}</td>
            </tr>
            <tr>
                <th>Int1</th>
                <td>{{ entity.int1 }}</td>
            </tr>
            <tr>
                <th>Int2</th>
                <td>{{ entity.int2 }}</td>
            </tr>
            <tr>
                <th>Dtcreation</th>
                <td>{{ entity.dtCreation|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('equipes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('equipes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('equipes_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
