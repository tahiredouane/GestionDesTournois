{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Equipes list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Association</th>
                <th>Adresse</th>
                <th>Coach</th>
                <th>Cin</th>
                <th>Photo</th>
                <th>Photoequipe</th>
                <th>Logo</th>
                <th>Lienphoto</th>
                <th>Lienphotoequipe</th>
                <th>Lienlogo</th>
                <th>Orders</th>
                <th>Str1</th>
                <th>Str2</th>
                <th>Str3</th>
                <th>Int1</th>
                <th>Int2</th>
                <th>Dtcreation</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('equipes_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nom }}</td>
                <td>{{ entity.association }}</td>
                <td>{{ entity.adresse }}</td>
                <td>{{ entity.coach }}</td>
                <td>{{ entity.cin }}</td>
                <td>{{ entity.photo }}</td>
                <td>{{ entity.photoEquipe }}</td>
                <td>{{ entity.logo }}</td>
                <td>{{ entity.lienPhoto }}</td>
                <td>{{ entity.lienPhotoEquipe }}</td>
                <td>{{ entity.lienLogo }}</td>
                <td>{{ entity.orders }}</td>
                <td>{{ entity.str1 }}</td>
                <td>{{ entity.str2 }}</td>
                <td>{{ entity.str3 }}</td>
                <td>{{ entity.int1 }}</td>
                <td>{{ entity.int2 }}</td>
                <td>{% if entity.dtCreation %}{{ entity.dtCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('equipes_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('equipes_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('equipes_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
